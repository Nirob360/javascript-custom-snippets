{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }




	"print const":{
		"prefix": "c",
		"body": [
			"const $1 ;"
		],
		"description": "Output const variable"

	},
	"print let":{
		"prefix": "l",
		"body": [
			"let $1 ;"
		],
		"description": "Output let variable"

	},	
	"print dir":{
		"prefix": "dir",
		"body": [
			"console.dir($1);"
		],
		"description": "consoel dir"

	},
	"Print Document.getElementById":{
		"prefix": "di",
		"body": [
			"document.getElementById('$1');"
		],
		"description": "type ElementById"
	},
	"Print Document.ByClassName":{
		"prefix": "dcn",
		"body": [
			"document.getElementsByClassName('$1');"
		],
		"description": "type getElementsByClassName"
	},
	
	"Print querySelector":{
		"prefix": "dq",
		"body": [
			"document.querySelector('$1');"
		],
		"description": "type querySelector"
	},
	
	"Print querySelectorAll":{
		"prefix": "dqa",
		"body": [
			"document.querySelectorAll('$1');"
		],
		"description": "type querySelectorAll"
	},


}
